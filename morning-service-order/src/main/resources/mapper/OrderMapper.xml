<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lucifer.order.dao.OrderMapper">


    <select id="selectAll" resultType="com.lucifer.order.pojo.Order">
        SELECT order_id,buyer_name,buyer_phone,buyer_address,order_amount,order_status, create_time,update_time
        from order_master
    </select>
</mapper>